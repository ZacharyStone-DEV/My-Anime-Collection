{
  "name": "myanimecollection",
  "version": "1.0.1",
  "description": "A anime tracking application",
  "main": "server/server.ts",
  "type": "module",
  "scripts": {
    "install-client": "cd client && npm install",
    "install-server": "npm install",
    "build-client": "cd client && npm run build",
    "install-dependencies": "npm run install-client && npm run install-server",
    "heroku-postbuild": "npm run install-dependencies && tsc && npm run build-client",
    "start-heroku-server": "  node dist/server/server.js",
    "server-local": "tsc && nodemon dist/server.js",
    "client-local": "npm start --prefix client",
    "start": "concurrently \"npm run server-local\" \"npm run client-local\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "nodemon": "^2.0.22",
    "concurrently": "^6.5.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-rate-limit": "^6.0.4",
    "helmet": "^5.0.1",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongoose": "^6.12.0",
    "morgan": "^1.10.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.18.3",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-simply-carousel": "^8.3.5",
    "react-toastify": "^9.0.8",
    "styled-components": "^5.3.5",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  }
}
